generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model gyerek{
  id Int @id @default(autoincrement())
  nev String
  cim String
  jo_volt Int

  jatekok jatek[]
}

model jatek{
  id Int @id @default(autoincrement())
  megnevezes String 
  anyag String 
  suly Int

  gyerekId Int
  gyerekek gyerek @relation(fields: [gyerekId], references: [id])
}
